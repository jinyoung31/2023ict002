inp = int(input(">:"))
list_inp=[inp]
​
for i in range(2, inp): # 2 ~ 2
        if inp % i == 0:
            list_inp.append(i)
            break
​
if len(list_inp) == 1 and inp != 1:
    print(list(list_inp))
    print("소수입니다")
        
else:
    print(list(list_inp))
    print("소수가 아닙니다.")
    
​
